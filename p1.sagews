def rr1(a, m):
    m = int(m); a = int(a)
    B = int(math.sqrt(m/2))
    for n in range(B+1):
        for d in range(-B, B+1):
            if d and (n - a*d)%m == 0:
                return ZZ(n)/d
    raise ValueError("no solution")
